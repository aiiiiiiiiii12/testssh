<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>Controller.LoginController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminLoginController</servlet-name>
        <servlet-class>Controller.AdminLoginController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HomePageAdminController</servlet-name>
        <servlet-class>Controller.HomePageAdminController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddRoomController</servlet-name>
        <servlet-class>Controller.AddRoomController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ManagermentUserController</servlet-name>
        <servlet-class>Controller.ManagermentUserController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminFeedbackManagement</servlet-name>
        <servlet-class>Controller.AdminFeedbackManagement</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminLoginController</servlet-name>
        <url-pattern>/loginadmin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HomePageAdminController</servlet-name>
        <url-pattern>/admincl</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddRoomController</servlet-name>
        <url-pattern>/addroom</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ManagermentUserController</servlet-name>
        <url-pattern>/usermanager</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminFeedbackManagement</servlet-name>
        <url-pattern>/feedback</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
